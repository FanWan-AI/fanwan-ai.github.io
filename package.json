{
  "name": "wanfan.github.io",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "scripts": {
  "blog:translate": "node scripts/translate-markdown.mjs",
    "og:render": "node scripts/render-all-og.mjs",
    "blog:build": "node scripts/generate-blog.mjs",
    "blog:build:one": "node scripts/generate-blog.mjs",
    "og:from-template": "node scripts/generate-og-from-template.mjs",
    "og:rasterize": "node scripts/rasterize-og.mjs",
    "blog:index": "node scripts/build-blog-index.mjs",
    "rss": "node scripts/generate-rss.mjs",
  "site:build": "npm run og:from-template && npm run og:rasterize || true && npm run og:render || true && npm run blog:translate && npm run blog:build && npm run blog:index && npm run rss && npm run daily:post",
  "serve": "npx http-server -p 8080 -c-1 .",
  "i18n:fill": "node tools/translate.mjs",
  "modelswatch:tag:dry": "python tools/modelswatch_tagging.py --dry-run",
  "modelswatch:tag": "python tools/modelswatch_tagging.py",
  "modelswatch:tag:llm": "python tools/modelswatch_tagging.py --use-llm",
  "daily:post": "npm run modelswatch:tag"
  },
  "devDependencies": {
    "puppeteer": "^22.15.0",
  "sharp": "^0.33.5",
  "http-server": "^14.1.1"
  }
}
